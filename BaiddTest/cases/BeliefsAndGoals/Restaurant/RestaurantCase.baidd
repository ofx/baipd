<?xml version="1.0" standalone="yes" ?>
<baidd-mas>
	<topic>safe</topic>
	<protocol>
		<deliberation-rules>
			<rule>AttackOnOwnMove</rule>
			<rule>NoRepeatInBranch</rule>
		</deliberation-rules>
		<termination-rules>
			<rule>NoParticipants</rule>
			<rule>InactiveRound</rule>
		</termination-rules>
	</protocol>
	<agents>
		<agent name="Olga" type="BDIAgent" file="Olga.agent" />
		<agent name="Paul" type="BDIAgent" file="Paul.agent" />
	</agents>
</baidd-mas>
