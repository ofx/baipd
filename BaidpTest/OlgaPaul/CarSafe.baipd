<?xml version="1.0" standalone="yes" ?>
<baipd-mas>
	<topic>safe</topic>
	<protocol>
		<persuasion-rules>
			<rule>AttackOnOwnMove</rule>
			<rule>NoRepeatInBranch</rule>
		</persuasion-rules>
		<termination-rules>
			<rule>NoParticipants</rule>
			<rule>InactiveRound</rule>
		</termination-rules>
	</protocol>
	<agents>
		<agent name="Olga" type="PersonalityAgent" file="Olga.agent" />
		<agent name="Paul" type="PersonalityAgent" file="Paul.agent" />
	</agents>
</baipd-mas>
